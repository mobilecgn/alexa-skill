{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "strict": 0,
    "class-methods-use-this": [0],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true
  }
}
